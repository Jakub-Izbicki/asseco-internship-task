<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<StackPane xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <GridPane>
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="40.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="40.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="40.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="40.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="40.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="40.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Text fx:id="addUserTitle" strokeType="OUTSIDE" strokeWidth="0.0" text="%addUserTitle"
                      GridPane.columnSpan="2" GridPane.halignment="CENTER">
                    <GridPane.margin>
                        <Insets/>
                    </GridPane.margin>
                    <font>
                        <Font name="System Bold" size="18.0"/>
                    </font>
                </Text>
                <TextField fx:id="addUsernameTextField" layoutX="130.0" layoutY="90.0" GridPane.columnIndex="1"
                           GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="CENTER">
                    <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </padding>
                    <GridPane.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </GridPane.margin>
                </TextField>
                <TextField fx:id="addFirstnameTextField" layoutX="110.0" layoutY="40.0" GridPane.columnIndex="1"
                           GridPane.halignment="CENTER" GridPane.rowIndex="2" GridPane.vgrow="NEVER">
                    <GridPane.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </GridPane.margin>
                    <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </padding>
                </TextField>
                <TextField fx:id="addLastnameTextField" layoutX="110.0" layoutY="70.0" GridPane.columnIndex="1"
                           GridPane.halignment="CENTER" GridPane.rowIndex="3" GridPane.vgrow="NEVER">
                    <GridPane.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </GridPane.margin>
                    <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </padding>
                </TextField>
                <TextField fx:id="addAgeTextField" layoutX="110.0" layoutY="100.0" GridPane.columnIndex="1"
                           GridPane.halignment="CENTER" GridPane.rowIndex="4" GridPane.vgrow="NEVER">
                    <GridPane.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </GridPane.margin>
                    <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </padding>
                </TextField>
                <TextField fx:id="addPasswordTextField" layoutX="130.0" layoutY="170.0" GridPane.columnIndex="1"
                           GridPane.halignment="CENTER" GridPane.rowIndex="5" GridPane.valignment="CENTER">
                    <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </padding>
                    <GridPane.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </GridPane.margin>
                </TextField>
                <Text fx:id="addPassword" layoutX="38.0" layoutY="196.0" strokeType="OUTSIDE" strokeWidth="0.0"
                      text="%addPassword" GridPane.halignment="CENTER" GridPane.rowIndex="5"
                      GridPane.valignment="CENTER">
                    <GridPane.margin>
                        <Insets/>
                    </GridPane.margin>
                </Text>
                <Text fx:id="addFirstnameText" layoutX="10.0" layoutY="61.0" strokeType="OUTSIDE" strokeWidth="0.0"
                      text="%addFirstnameText" GridPane.halignment="CENTER" GridPane.rowIndex="2"
                      GridPane.valignment="CENTER">
                    <GridPane.margin>
                        <Insets/>
                    </GridPane.margin>
                </Text>
                <Text fx:id="addLastnameText" layoutX="10.0" layoutY="91.0" strokeType="OUTSIDE" strokeWidth="0.0"
                      text="%addLastnameText" GridPane.halignment="CENTER" GridPane.rowIndex="3"
                      GridPane.valignment="CENTER">
                    <GridPane.margin>
                        <Insets/>
                    </GridPane.margin>
                </Text>
                <Text fx:id="addAge" layoutX="10.0" layoutY="121.0" strokeType="OUTSIDE" strokeWidth="0.0"
                      text="%addAge" GridPane.halignment="CENTER" GridPane.rowIndex="4" GridPane.valignment="CENTER">
                    <GridPane.margin>
                        <Insets/>
                    </GridPane.margin>
                </Text>
                <Button fx:id="cancelAddButton" mnemonicParsing="false" onAction="#onCancelAddPButtonPress"
                        text="%cancelAddButton" GridPane.halignment="CENTER" GridPane.rowIndex="7"
                        GridPane.valignment="CENTER">
                    <GridPane.margin>
                        <Insets/>
                    </GridPane.margin>
                </Button>
                <Button fx:id="saveAddButton" mnemonicParsing="false" onAction="#onSaveAddButtonPress"
                        text="%saveAddButton" GridPane.columnIndex="1" GridPane.halignment="CENTER"
                        GridPane.rowIndex="7" GridPane.valignment="CENTER">
                    <GridPane.margin>
                        <Insets/>
                    </GridPane.margin>
                </Button>
                <Text fx:id="addUserErrorInfo" strokeType="OUTSIDE" strokeWidth="0.0" GridPane.columnSpan="2"
                      GridPane.halignment="CENTER" GridPane.rowIndex="6" GridPane.valignment="CENTER"/>
                <Text fx:id="addUsernameText" layoutX="5.0" layoutY="116.0" strokeType="OUTSIDE" strokeWidth="0.0"
                      text="%addUsernameText" GridPane.halignment="CENTER" GridPane.rowIndex="1"
                      GridPane.valignment="CENTER"/>
            </children>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
            <StackPane.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </StackPane.margin>
        </GridPane>
    </children>
</StackPane>
